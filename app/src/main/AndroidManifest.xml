<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="desk713.hackathon.findersandkeepers">

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".SocketHandler"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TakePictureActivity" />
        <activity android:name=".AddDescriptionActivity" />
        <activity android:name=".SecurityQuestionsActivity" />
        <activity android:name=".ContactDetailsActivity" />
        <activity android:name=".DepositActivity" />
        <activity android:name=".AskDetails" />
        <activity android:name=".ListItems" />
        <activity android:name=".CheckAnswersActivity" />
        <activity android:name=".GetAccessActivity" />
        <activity android:name=".FinalActivity"></activity>
    </application>

</manifest>